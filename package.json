{
  "name": "jktota",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec ts-node server/index.ts",
    "dev:client": "cd client && npm run dev",
    "dev:telegram": "cd telegram-bot && python bot.py",
    "dev:youtube": "cd youtube-service && python main.py",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:all": "concurrently -n \"server,client,telegram,youtube\" -c \"blue,green,yellow,magenta\" \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:telegram\" \"npm run dev:youtube\"",
    "build:server": "tsc",
    "build:client": "cd client && npm run build",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@doist/todoist-api-typescript": "^6.2.1",
    "axios": "^1.13.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
